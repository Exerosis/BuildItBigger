<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>me.exerosis.builditbigger.RetrofitServlet</servlet-class>
        <init-param>
            <param-name>retrofit_interface</param-name>
            <param-value>me.exerosis.builditbigger.JokeTestStore</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/api</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>